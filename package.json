{
  "name": "apollo-frontpage-angular2-app",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "remove-mutation": "node ./scripts/remove-mutation.js",
    "build": "ng build",
    "build:query": "npm run remove-mutation && npm run build -- --base-href /frontpage-angular2-app-query/",
