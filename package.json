    "deploy": "sh deploy.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "@types/chai": "^3.4.34",
    "angular2-apollo": "0.8.0",
    "apollo-client": "0.5.1",
    "core-js": "^2.4.0",
    "graphql": "^0.7.0",
    "graphql-subscriptions": "^0.2.1",
    "graphql-tag": "^0.1.16",
    "graphql-tools": "^0.8.1",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.22"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.30",
    "@types/node": "^6.0.38",
    "angular-cli": "1.0.0-beta.11-webpack.8",
    "codelyzer": "~0.0.26",
    "replace": "^0.3.0",
